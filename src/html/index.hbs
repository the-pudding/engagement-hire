<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>

	{{> base/meta}}

	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}

</head>

<body>

	{{> base/header 'sticky'}}

	<main id='content'>

		<div class='intro'>
			<p class="closed">Applications are closed</p>
			<h1 class='intro__hed'>{{{doc.title}}}</h1>
			<time class='intro__time'>April 2022</time>
			<div class="text-section">
				{{#each doc.introSection}}
					<p>{{{value}}}</p>
				{{/each}}
			</div>

			<h3 class='section__hed'>{{doc.usHed}}</h3>
			<div class="text-section">
				{{#each doc.usSection}}
					{{#ifEquals type 'ul'}}
					{{#each value}}
						<li>{{{this}}}</li>
					{{/each}}
					{{/ifEquals}}
					{{#ifEquals type 'text'}}
						<p>{{{value}}}</p>
					{{/ifEquals}}
				{{/each}}
			</div>

			<h3 class='section__hed'>{{doc.responsibilitiesHed}}</h3>
			<div class="text-section">
				{{#each doc.responsibilitiesSection}}
					{{#ifEquals type 'ul'}}
					{{#each value}}
						<li>{{{this}}}</li>
					{{/each}}
					{{/ifEquals}}
					{{#ifEquals type 'text'}}
						<p>{{{value}}}</p>
					{{/ifEquals}}
				{{/each}}
			</div>

			<h3 class='section__hed'>{{doc.skillsHed}}</h3>
			<div class="text-section">
				{{#each doc.skillsSection}}
					{{#ifEquals type 'ul'}}
					{{#each value}}
						<li>{{{this}}}</li>
					{{/each}}
					{{/ifEquals}}
					{{#ifEquals type 'text'}}
						<p>{{{value}}}</p>
					{{/ifEquals}}
				{{/each}}
			</div>

			<h3 class='section__hed'>{{doc.hereHed}}</h3>
			<div class="text-section">
			
				{{#each doc.hereSection}}
					{{#ifEquals type 'ul'}}
					{{#each value}}
						<li>{{{this}}}</li>
					{{/each}}
					{{/ifEquals}}
					{{#ifEquals type 'text'}}
						<p>{{{value}}}</p>
					{{/ifEquals}}
				{{/each}}
			</div>
		
			<h3 class='section__hed'>{{doc.compHed}}</h3>
			<div class="text-section">
		
				{{#each doc.compSection}}
					{{#ifEquals type 'ul'}}
					{{#each value}}
						<li>{{{this}}}</li>
					{{/each}}
					{{/ifEquals}}
					{{#ifEquals type 'text'}}
						<p>{{{value}}}</p>
					{{/ifEquals}}
				{{/each}}
			</div>
			<div class="text-section">
				{{#each doc.applySection}}
					{{#ifEquals type 'ul'}}
					{{#each value}}
						<li>{{{this}}}</li>
					{{/each}}
					{{/ifEquals}}
					{{#ifEquals type 'text'}}
						<p>{{{value}}}</p>
					{{/ifEquals}}
				{{/each}}
			</div>
		
		</div>


	</main>

	{{> base/footer }}


	<script src='{{basepath}}assets/scripts/d3.v6.0.0.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>